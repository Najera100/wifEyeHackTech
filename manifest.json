{
  "manifest_version": 2,
  "name": "Wif-eye",
  "description": "This extension will be you beloved privacy guard",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.gif",
    "default_popup_dontuseit": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ]
}